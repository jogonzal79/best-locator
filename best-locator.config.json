{
  "defaultFramework": "playwright",
  "defaultLanguage": "python",
  "projectAttributes": [
    "data-testid",
    "data-cy",
    "data-test",
    "data-qa"
  ],
  "urls": {
    "local": "http://localhost:3000",
    "google": "https://www.google.com",
    "github": "https://github.com",
    "staging": "https://staging.myapp.com"
  },
  "browser": {
    "headless": false,
    "viewport": {
      "width": 1280,
      "height": 720
    }
  },
  "output": {
    "includeConfidence": true,
    "includeXPath": false
  },
  "timeouts": {
    "pageLoad": 30000,
    "elementSelection": 3600000,
    "validation": 15000
  },
  "appium": {
    "enabled": true,
    "capabilities": {
      "android": {
        "platformName": "Android",
        "appium:automationName": "UiAutomator2",
        "appium:udid": "Power51280021852",
        "appium:platformVersion": "11",
        "appium:newCommandTimeout": 600        
      }
    }
  },
  "ai": {
    "enabled": true,
    "provider": "openai",
    "ollama": {
      "host": "http://localhost:11434",
      "model": "llama3.1:8b",
      "timeout": 60000,
      "temperature": 0.3
    },
    "openai": {
      "apiKey": null,
      "model": "gpt-4o",
      "timeout": 20000,
      "temperature": 0.1
    },
    "features": {
      "smartSelector": true,
      "explainDecisions": true
    },
    "fallback": {
      "onError": "traditional"
    }
  }
}